{
  "extends": "airbnb-base",
  "globals": {
    "it": true,
    "describe": true,
    "expect": true
  },
  "rules": {
    "arrow-parens": [
      "error",
      "always"
    ],
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}
